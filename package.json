{
  "name": "space-pilot-coach",
  "version": "0.0.0",
  "description": "Since it is our primary goal to achieve shit, then we do so.",
  "author": "Creō, Jūstitia, Reconnaître, Kármán, and Scandō of Thailand Space Pilot training.",
  "bin": {
    "space-pilot-coach": "./bin/run"
  },
  "homepage": "https://github.com/intaniger/space-pilot-coach",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "intaniger/space-pilot-coach",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@discordjs/rest": "^0.4.1",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@types/chance": "^1.1.3",
    "@types/mongoose": "^5.11.97",
    "@types/qrcode": "^1.4.2",
    "@types/uuid": "^8.3.4",
    "axios": "^0.27.2",
    "bluebird": "^3.7.2",
    "chance": "^1.1.8",
    "dayjs": "^1.11.2",
    "dedent": "^0.7.0",
    "discord-api-types": "^0.33.2",
    "discord.js": "^13.7.0",
    "google-it": "^1.6.2",
    "keyv": "^4.3.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.3.5",
    "puppeteer": "^14.3.0",
    "qrcode": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "type-fest": "^2.13.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/dedent": "^0.7.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.31.0",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "space-pilot-coach",
    "dirname": "space-pilot-coach",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "dev": "shx rm -rf dist && tsc -b && ./bin/run serve --token"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/intaniger/space-pilot-coach/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
